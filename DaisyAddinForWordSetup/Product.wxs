			<Publish Dialog="ExitDialog" Control="Finish" Event="DoAction"  Value="LaunchApplication">WIXUI_EXITDIALOGOPTIONALCHECKBOX = 1 and NOT Installed</Publish>
		</UI>-->
        <!--Custom UI-->
        <UIRef Id="MyUI" />
        <!--Custom UI-->
        <!--<Property Id="WIXUI_EXITDIALOGOPTIONALCHECKBOXTEXT" Value="Show Help" />-->
        <WixVariable Id="WixUIBannerBmp" Value="$(var.ProjectDir)images\bannrbmp.bmp" />
        <WixVariable Id="WixUIDialogBmp" Value="$(var.ProjectDir)images\dlgbmp.bmp" />
        <WixVariable Id="WixUILicenseRtf" Value="$(var.ProjectDir)License.rtf" />
        <!--Custom Action-->
        <CustomAction Id="NewerVersionDetected" Error="There is a newer version of this product installed." />
        <!--Built in UI-->
        <CustomAction Id="ShowReadme" BinaryKey="WixCA" DllEntry="WixShellExec" Impersonate="yes" />
        <!--Get latest word version-->
        <CustomAction BinaryKey="DaisyAddinCustomActions" DllEntry="DetectLatestMsWordVersion" Execute="immediate" Id="GetLatestMsWordVersion" />
        <InstallExecuteSequence>
            <!-- Prevent downgrading -->
            <Custom Action="NewerVersionDetected" After="FindRelatedProducts">NEWAPPFOUND</Custom>
            <Custom Action="SetARPREADME" After="InstallValidate">
            </Custom>
            <Custom Action="GetLatestMsWordVersion" Before="LaunchConditions">NOT Installed</Custom>
            <RemoveExistingProducts Before="InstallInitialize" />
        </InstallExecuteSequence>
        <UI />
        <Binary Id="DaisyAddinCustomActions" SourceFile="..\DaisyAddin.CustomActions\bin\Debug\DaisyAddin.CustomActions.CA.dll" />
    </Product>
</Wix>